  CFLAGS     = -O3 -s -DNDEBUG --no-multiarch -march=native -mtune=native -Wno-macro-redefined -Wno-unused-variable -Wno-unused-function -Wno-unused-local-typedefs -Wno-unknown-pragmas -Wno-c++11-inline-namespace
